.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 76px;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  overflow: visible!important;
	background-color: #ffffff;
	box-shadow: 0px -3px 6px 0px 
		rgba(0, 0, 0, 0.13);
  border-radius: 24px 24px 0px 0px;
  z-index: -1;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.tab-bar-item-1 {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  overflow: visible!important;
 
}
.tab-bar-item-1 image {
  position: relative;
  top: -30px;
  width: 53px;
  height: 53px;
  overflow: visible!important;
  display: inline;
  
}
.top-img{
  width: 60px;
  height: 60px;
}
.top-dev{
  width: 100%;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  position: absolute;
  padding-bottom: calc(env(safe-area-inset-bottom) + 30px);
  z-index: 99;
}
.tab-bar-item-1 div {
  font-size: 10px;
  margin-top: -25px;
}
.tab-bar-item image {
  width: 28px;
  height: 28px;
}

.tab-bar-item div {
  font-size: 10px;
}
.background{
  /* position: absolute; */
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: black;
  opacity:0.5;
  z-index: 91;
  display: flex;
  justify-content: center;
}
.click-top{
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  position: fixed;
  left: 0;
  padding-bottom: env(safe-area-inset-bottom);
  padding-bottom: calc(env(safe-area-inset-bottom) + 66px);
  z-index: 99;
}

.item{
  width: 165px;
	height: 129px;
	background-color: #ffffff;
  border-radius: 8px;
  font-size: 28px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 32px;
	letter-spacing: 0px;
  color: #53606f;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin:0 8px;
}

.background image{
  width: 53px;
  height: 53px;
}
.item image{
  width: 34px;
	height: 42px;
}
.click-top-all{
  align-items: center;
}